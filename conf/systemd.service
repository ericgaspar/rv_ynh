[Unit]
Description=streamhut
Documentation=
After=network.target

[Service]
Type=simple
User=__APP__
Group=__APP__
WorkingDirectory=__FINALPATH__
Environment="PATH=__ENV_PATH__"
Environment="NODE_ENV=production"
ExecStart=__YNH_NPM__ HOST=127.0.0.1 PORT=3050 SKIP_PREFLIGHT_CHECK=true BROWSER=none react-scripts --max-old-space-size=4096 start
Restart=always
PrivateTmp=true
PrivateDevices=true

[Install]
WantedBy=multi-user.target

